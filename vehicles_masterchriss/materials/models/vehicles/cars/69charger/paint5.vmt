"VertexLitGeneric"
{
	"$basetexture"	"Models/vehicles/cars/69charger/paint_Black"
	"$bumpmap"	      "Models/vehicles/cars/69charger/paint_normal"
	
	"$halflambert" 1
	"$phong" "1" //this turns on the phong effect
	"$phongboost" "25" //use this to increase/decrease the highlight intensity 
	"$phongfresnelranges" "[0.5 0.8 5]" //this are the highlight intensities at different incidence angles from the camera (first value is perpendicular to the camera and the 3rd value is away from the camera)
	"$phongexponent"  "250"
	//"$phongtint" "[1 0 0]"
    //"$PhongWarpTexture" "Models/vehicles/cars/69charger/MetallicWarpRed2"

	"$envmap" "env_cubemap"
	//"$envmaptint" "[0.3 0.3 0.4]"
	
	!hdr?$envmaptint "[ 0.3 0.3 0.4 ]"
	hdr?$envmaptint "[ 0.2 0.2 0.25 ]"
	
	"$envmapsaturation" "0.0"
	"$normalmapalphaenvmapmask" 1
	"$envmapFresnel" "1"
}
