    //"$rimlight" "1"
	//"$rimlightexponent" "250"
	//"$rimlightboost" "0.5"
	//"$rimmask" "0"

	"$envmap" "env_cubemap"
	//"$envmaptint" "[0.4 0.4 0.7]"
	
	!hdr?$envmaptint "[ 0.4 0.4 0.7 ]"
	hdr?$envmaptint "[ 0.25 0.25 0.4 ]"
	
	"$normalmapalphaenvmapmask" 1
	"$envmapFresnel" "1"
	
	$detail "models/vehicles/cars/69charger/metallic"
	$detailscale "35"
	$detailblendfactor "0.15"
	$detailblendmode "0"
}
