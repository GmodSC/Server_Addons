"VertexLitGeneric"
{
	"$basetexture"	"Models/vehicles/cars/69charger/paint_Sky_Blue"
	"$bumpmap"	      "Models/vehicles/cars/69charger/paint_normal"
	
	"$halflambert" 1
	"$phong" "1" //this turns on the phong effect
	"$phongboost" "8" //use this to increase/decrease the highlight intensity 
	"$phongfresnelranges" "[0.5 0.8 5]" //this are the highlight intensities at different incidence angles from the camera (first value is perpendicular to the camera and the 3rd value is away from the camera)
	"$phongexponent"  "15"
	"$phongtint" "[0 0.5 0.45]"
